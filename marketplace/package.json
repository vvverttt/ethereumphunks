    "ng-lazyload-image": "^9.1.3",
    "ngx-pagination": "^6.0.3",
    "ngx-socket-io": "^4.8.5",
    "ngx-timeago": "^3.0.0",
    "ngxtension": "^5.0.0",
    "react": "^18.3.1",
    "rxjs": "^7.4.0",
    "svgson": "^5.3.1",
