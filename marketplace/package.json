{
  "name": "etherphunks-market",
  "version": "2.1.0",
  "type": "module",
  "scripts": {
    "prebuild": "node prebuild.js",
    "ng": "ng",
    "start:sepolia": "ng serve --configuration dev-sepolia --host 0.0.0.0 --port 4200",
    "start:mainnet": "ng serve --configuration dev-mainnet --host 0.0.0.0 --port 4200",
    "start:sepolia:9000": "ng serve --configuration dev-sepolia --host 0.0.0.0 --port 9000",
    "start:mainnet:9000": "ng serve --configuration dev-mainnet --host 0.0.0.0 --port 9000",
    "build": "yarn build:sepolia && yarn build:mainnet",
    "build:sepolia": "yarn prebuild && ng build --configuration sepolia",
    "build:mainnet": "yarn prebuild && ng build --configuration mainnet",
    "test": "ng test",
    "run-built": "http-server dist/etherphunks-market -p 8000",
    "translate": "ng extract-i18n --output-path src/locale",
    "translate:json": "ng extract-i18n --output-path src/locale --format=json",
    "deploy:ipfs:sepolia": "node deploy-ipfs.js --configuration=sepolia",
    "deploy:ipfs:mainnet": "node deploy-ipfs.js --configuration=mainnet",
    "deploy:ipfs": "yarn deploy:ipfs:mainnet && yarn deploy:ipfs:sepolia"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.2.9",
    "@angular/common": "19.2.9",
    "@angular/compiler": "19.2.9",
    "@angular/core": "19.2.9",
    "@angular/forms": "19.2.9",
    "@angular/platform-browser": "19.2.9",
    "@angular/platform-browser-dynamic": "19.2.9",
    "@angular/router": "19.2.9",
    "@angular/service-worker": "19.2.9",
    "@apollo/client": "^3.13.8",
    "@ng-select/ng-select": "^14.7.0",
    "@ng-web-apis/common": "^4.12.0",
    "@ng-web-apis/intersection-observer": "^4.12.0",
    "@ngrx/effects": "^19.1.0",
    "@ngrx/operators": "^19.1.0",
    "@ngrx/router-store": "^19.1.0",
    "@ngrx/store": "^19.1.0",
    "@ngrx/store-devtools": "^19.1.0",
    "@supabase/supabase-js": "^2.47.16",
    "@wagmi/connectors": "^5.8.1",
    "@wagmi/core": "^2.17.1",
    "@web3modal/wagmi": "^5.1.11",
    "@xmtp/xmtp-js": "^11.4.1",
    "animejs": "^3.2.1",
    "apollo-angular": "^10.0.3",
